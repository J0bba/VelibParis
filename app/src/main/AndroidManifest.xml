<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projet.mti.velibparis">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_directions_bike_black_24dp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_directions_bike_black_24dp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="android.app.searchable"
            android:resource="@xml/searchable" />
        <activity android:name=".Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Detail.DetailsActivity"
            android:parentActivityName=".Main.MainActivity">

        </activity>
        <activity android:name=".GroupDetails.GroupDetailsActivity"
            android:parentActivityName=".Main.MainActivity">

        </activity>
    </application>

</manifest>